        "/api/occurrences": {
            "get": {
                "tags": [
                    "Occurrences"
                ],
                "summary": "Listar ocorrências",
                "description": "Retorna uma lista paginada de ocorrências com opções de filtro por status e tipo",
                "operationId": "listOccurrences",
                "parameters": [
                    {
                        "name": "status",
                        "in": "query",
                        "description": "Filtrar por status da ocorrência",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "pending"
                        }
                    },
                    {
                        "name": "type",
                        "in": "query",
                        "description": "Filtrar por tipo de ocorrência",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "example": "incendio_urbano"
                        }
                    },
                    {
                        "name": "limit",
                        "in": "query",
                        "description": "Número de itens por página",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 50,
                            "maximum": 100,
                            "minimum": 1
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número da página",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1,
                            "minimum": 1
                        }
                    }
